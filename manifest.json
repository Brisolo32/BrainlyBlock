{
    "name": "Brainly Block",
    "description": "Bloqueia o PayWall do Brainly",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "activeTab",
        "scripting",
        "http://*/",
        "https://*/"
    ]
}