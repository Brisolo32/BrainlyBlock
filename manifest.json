{
    "name": "Brainly Block",
    "description": "Bloqueia o PayWall do Brainly",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://*.brainly.com.br/*"],
            "js": ["background.js"]
        }
    ]
}
